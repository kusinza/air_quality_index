<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/settings_backgroung">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#f1b2b2" />
            <solid android:color="@color/blurry" />
            <corners android:radius="160dp"/>
        </shape>
    </item>


    <item
        android:top="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/hint_color1" />
            <corners android:radius="180dp"/>
            <solid android:color="#00fafafa"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ambulance"/>






</layer-list>